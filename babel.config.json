{
    "presets": [
        "@babel/preset-env",
        "@babel/preset-typescript",
        [
            "@babel/preset-react",
            {
                "runtime": "automatic"
            }
        ]
    ],
    "plugins": [
        [
            "i18next-extract",
            {
                "keyAsDefaultValue": true,
                "outputPath": "extractedTranslations/{{locale}}/{{ns}}.json",
                "locales": [
                    "en",
                    "ru"
                ]
            }
        ]
    ]
}